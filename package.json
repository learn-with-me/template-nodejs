{
  "name": "template-nodejs",
  "version": "1.0.0",
  "description": "Library template",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.js",
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint"
    ]
  },
  "scripts": {
    "prettier": "prettier '**/*.{js,ts}' --ignore-path ./.prettierignore",
    "types": "tsc --noEmit",
    "lint": "eslint '*/**/*.{js,ts}'",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "rollup -c",
    "finish": "npm run lint && npm run types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goel4ever/template-nodejs.git"
  },
  "browserslist": [
    "last 2 version "
  ],
  "author": "Anshul Goel <dec.anshul@gmail.com>",
  "license": "MIT",
  "keywords": [
    "template",
    "javascript",
    "package"
  ],
  "bugs": {
    "url": "https://github.com/goel4ever/template-nodejs/issues"
  },
  "homepage": "https://github.com/goel4ever/template-nodejs#readme",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.6.3"
  }
}
